@using runnerDotNet;
@{
 XTempl Model = this.Model;
 if(ViewBag != null && ViewBag.xt != null)
 {
  Model = ViewBag.xt;
  if(Model != null)
  {
   Model.currentViewContext = ViewContext;
  }
 }
 string webRootPath = MVCFunctions.GetWebRootPath();
}

<!doctype html>@(new HtmlString(MVCFunctions.GetBuferContentAndClearBufer()))
<HTML @(Model.displayVar("html_attrs"))>
	<head>
		<META http-equiv="X-UA-Compatible" content="IE=Edge" />
		<META name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>@(Model.displayVar("pagetitle"))</title>

		<META http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<LINK REL="stylesheet" href="@(webRootPath)styles/default.css" type="text/css" />
		@foreach (string _rtlCSS in Model.GetSection("rtlCSS", this)){
		<LINK REL="stylesheet" href="@(webRootPath)styles/defaultRTL.css" type="text/css" />
		}
		@foreach (string _styleCSSFiles in Model.GetSection("styleCSSFiles", this)){
		<LINK REL="stylesheet" href="@(webRootPath)@(Model.displayVar("stylepath"))" type="text/css" />
		}
	</head>

	<BODY class="@(Model.displayVar("stylename")) function-rprint" @(Model.displayVar("bodyattrs"))>
	@foreach (string _body in Model.GetSection("body", this)){
		{Html.RenderSharedView("header");}
		<style>@MvcHtmlString.Create(@"")@(Model.displayVar("containerCss"))@MvcHtmlString.Create(@"")</style>
		
		<div class="container r-small-page" 
			data-body-align="center"
			data-body-width="standard"
		>
							@foreach (string _top_block in Model.GetSection("top_block", this)){
<DIV class="r-form" data-location="top" id="form_top_@(Model.displayVar("pageid"))" data-makeup="small" @(Model.displayVar("form_top")) data-pageid="@(Model.displayVar("pageid"))">		
	 
		<div class="row">
			 
				<DIV data-cellId="top_c2" data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("cell_top_c2")) class=" r-ori-vert			col-md-12
	
">
		 
		<H2 data-itemtype="print_header" data-itemid="print_header" @(Model.displayVar("item_print_header")) data-pageid="@(Model.displayVar("pageid"))">@(Model.displayVar("pagetitlelabel Practice_Times rprint"))</h2>
	 
		@foreach (string _printtabheader in Model.GetSection("printtabheader", this)){
<SPAN data-itemtype="print_subheader" data-itemid="print_subheader" @(Model.displayVar("item_print_subheader")) data-pageid="@(Model.displayVar("pageid"))">
@(Model.displayVar("printtabheader_text"))
</span>
}
		</div>					</div>
	</div>		
}
									@foreach (string _above_grid_block in Model.GetSection("above-grid_block", this)){
<DIV class="r-form" data-location="above-grid" id="form_above-grid_@(Model.displayVar("pageid"))" data-makeup="small" @(Model.displayVar("form_above-grid")) data-pageid="@(Model.displayVar("pageid"))">		
	 
		<div class="row">
			 
				<DIV data-cellId="above-grid_c1" data-pageid="@(Model.displayVar("pageid"))" @(Model.displayVar("cell_above-grid_c1")) class=" r-ori-vert			col-md-12
	
">
		@foreach (string _firstAboveGridCell in Model.GetSection("firstAboveGridCell", this)){
		 
		<SPAN data-itemtype="print_pages" data-itemid="print_pages" @(Model.displayVar("item_print_pages")) data-pageid="@(Model.displayVar("pageid"))">
	@(Model.displayVar("print_pages_labelprint_pages"))
</span>			}	
	</div>					</div>
	</div>		
}
									@foreach (string _grid_block in Model.GetSection("grid_block", this)){
<style>@MvcHtmlString.Create(@"
	td.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary {
		background-color: #ffffbb;
	}
	td.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary {background-color: #ffe0b0;
	}
	.rnr-report {
		width: 100%;
	}
	td {
		padding: 2px; 4px;
	}
			td.group_1{
			background-color: #FFFFFF;
		}
		td.nbgroup_1{
			background-color: #FFFFFF;
		}
		td.header_1 {
			background-color: #D8D8D8; 
			font-size: 120%;
			padding: 2px 8px;
			font-weight: bold;
		}
			td.group_2{
			background-color: #FFFFFF;
		}
		td.nbgroup_2{
			background-color: #FFFFFF;
		}
		td.header_2 {
			background-color: #D8D8D8; 
			font-size: 120%;
			padding: 2px 8px;
			font-weight: bold;
		}
		td.blackshade {
		font-size: 120%;
	}
	.summaryrow > * {
		border:solid #808080 1px; 
		border-left: none; 
		border-right: none;
	}
		.grouptotalsrow + :not(.grouptotalsrow) > * {
		border-top:solid #808080 1px; 
	}")</style>
<DIV id="grid_block@(Model.displayVar("id"))" class="rnr-report-gridblock">
						
 <!-- add to settings ? -->



<table class="table table-striped table-bordered rnr-b-grid rnr-gridtable" cellpadding="0" >
	<tbody>
	@foreach (string _grid_row in Model.GetSection("grid_row", this)){
	 
		foreach (string _practice_time_newgroup in Model.GetSection("practice_time_newgroup", this)){
				<tr class="group-headers">
										<td class="blackshade">@(Model.displayVar("label Practice_Times practice_time"))</td>
														<td class="blackshade">@(Model.displayVar("label Practice_Times swimmer"))</td>
							
																						
		</tr>
						
		<tr class="group-spliter-1"  >
															<td colspan="2" class="header_1">
				<font >@(Model.displayVar("practice_time_grval"))</font>
			</td>
																					</tr>
		}
	 
		foreach (string _swimmer_newgroup in Model.GetSection("swimmer_newgroup", this)){
						
		<TR class="group-spliter-2" @(Model.displayVar("rowattrs"))>
										<td class=" group_2" >&nbsp;</td>					
																			<td colspan="1" class="header_2">
				<font>@(Model.displayVar("swimmer_grval"))</font>
			</td>
														</tr>
		}
	
		<!--  data-->
		foreach (string _row_data in Model.GetSection("row_data", this)){
		<tr>
								<td class=" group_1" >&nbsp;</td>
											<td class=" group_2" >&nbsp;</td>
																			</tr>
		}
	
				
	foreach (string _swimmer_endgroup in Model.GetSection("swimmer_endgroup", this)){
		<tr class="summaryrow" style="display:none; height:0px;" >
							 
			<td class="group_1" style="border-top: none;"></td> 
					
				

																	<td  class="group_2" colspan=1 >
				Summary for @(Model.displayVar("label Practice_Times swimmer")) @(Model.displayVar("swimmer_grval"))
					- <SPAN class="summary-2" data-type="summary" data-value="@(Model.displayVar("groupswimmer_total_cnt"))">@(Model.displayVar("groupswimmer_total_cnt"))
					</span> records total			</td>
									</tr>

	
	
	
	
	}
						
	foreach (string _practice_time_endgroup in Model.GetSection("practice_time_endgroup", this)){
		<tr class="summaryrow"  >
				

										<td  class="group_1" colspan=2 >
				Summary for @(Model.displayVar("label Practice_Times practice_time")) @(Model.displayVar("practice_time_grval"))
					- <SPAN class="summary-1" data-type="summary" data-value="@(Model.displayVar("grouppractice_time_total_cnt"))">@(Model.displayVar("grouppractice_time_total_cnt"))
					</span> records total			</td>
																</tr>

	
	
	
	
	}
			
	}


	
	</tbody>
</table>


			</div>
}									@foreach (string _below_grid_block in Model.GetSection("below-grid_block", this)){
<DIV class="r-form" data-location="below-grid" id="form_below-grid_@(Model.displayVar("pageid"))" data-makeup="small" @(Model.displayVar("form_below-grid")) data-pageid="@(Model.displayVar("pageid"))">		
	</div>		
}
			</div>
		{Html.RenderSharedView("footer");}
	}
	</body>
</html>
